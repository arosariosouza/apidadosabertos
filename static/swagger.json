                "parameters": [
                    {
                        "name": "codigo_tipo_unidade",
                        "in": "path",
                        "description": "código do tipo de unidade",
                        "required": true,
                        "type": "integer",
                        "schema" : {
                            "$ref": "#/components/schemas/codigo_tipo_unidade"
                        }
                    }
                ],               
                "responses": {
